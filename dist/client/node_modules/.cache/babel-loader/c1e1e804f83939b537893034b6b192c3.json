te>\r\n  <v-sheet\r\n      @scroll=\"callback\"\r\n      color=\"background\"\r\n      id=\"scrolling-techniques-2\"\r\n      :class=\"`overflow-y-${overflowYMode} overflow-x-hidden`\"\r\n      height=\"792px\"\r\n  >\r\n    <div>\r\n    </div>\r\n    <div :style=\"`background: #601a8f;color: white;width: 100%;height: ${this.height}px`\">\r\n    </div>\r\n    <div class=\"carousel__wrapper\" :style=\"`height: ${carouselHeight}px`\">\r\n      <transition appear\r\n        name=\"bounce\">\r\n        <carousel\r\n          v-if=\"presentationCarouselDrawer\"\r\n          :height=\"carouselHeight\"\r\n          :page-name=\"pageName\"\r\n          component-name=\"presentation\"/>\r\n      </transition>\r\n    </div>\r\n    <div class=\"carousel__wrapper\" :style=\"`height: ${height}px;`\">\r\n      <carousel\r\n        :height=\"carouselHeight\"\r\n        :page-name=\"pageName\"\r\n        component-name=\"presentation\"/>\r\n    </div>\r\n  </v-sheet>\r\n</template>\r\n\r\n<script>\r\nimport TheIndexViewCarousel from \"@/components/carousels/Carousel.vue\";\r\nimport velocityMixin from \"@/mixins/velocityMixin\";\r\nimport Carousel from \"@/components/carousels/Carousel.vue\";\r\n\r\nexport default {\r\n  name: \"TheContent\",\r\n  data: () => ({\r\n    presentationCarouselDrawer: true\r\n  }),\r\n  components: {Carousel, TheIndexViewCarousel},\r\n  props: {\r\n    height: {\r\n      type: Number,\r\n      required: true\r\n    },\r\n    pageName: {\r\n      type: String,\r\n      required: true\r\n    },\r\n  },\r\n  methods: {\r\n    callback() {\r\n      this.presentationCarouselDrawer =\r\n          document.getElementsByClassName(\"carousel__wrapper\")[0]\r\n              .getBoundingClientRect().y * 100 / this.$vuetify.breakpoint.height > 1;\r\n    }\r\n  },\r\n  computed: {\r\n    carouselHeight() {\r\n      return this.$vuetify.breakpoint.height * 0.573;\r\n    },\r\n    overflowYMode() {\r\n      switch (this.$vuetify.breakpoint.name) {\r\n        case \"lg\":\r\n          case \"xl\":\r\n            return \"auto\";\r\n        default:\r\n          return \"hidden\";\r\n      }\r\n    }\r\n  },\r\n  mixins: [velocityMixin]\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}